{
  "name": "express-security",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:client": "webpack --progress --colors -p",
    "build:images": "imagemin ./client/images/* ./public/images/",
    "lint": "eslint . --ext .js,.pug",
    "lint:tests": "eslint ./tests --ext .js",
    "start": "node ./bin/start.js",
    "test": "npm-run-all -s test:*",
    "test:unit": "jasmine-node ./tests/unit --color --verbose --captureExceptions",
    "test:e2e": "jasmine-node ./tests/e2e --color --verbose --captureExceptions",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "nodemon ./bin/start.js",
    "watch:client": "webpack --progress --colors --watch"
  },
  "dependencies": {
    "body-parser": "1.17.0",
    "cache-manager": "2.4.0",
    "cache-manager-redis": "0.4.0",
    "compression": "1.6.2",
    "connect-redis": "3.2.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.6.1",
    "express": "4.15.0",
    "express-rate-limit": "2.6.0",
    "express-session": "1.15.1",
    "express-socket.io-session": "1.3.2",
    "extend": "3.0.0",
    "find": "0.2.7",
    "helmet": "3.4.1",
    "jquery": "3.1.1",
    "mime-types": "2.1.14",
    "moment": "2.17.1",
    "morgan": "1.8.1",
    "ms": "0.7.2",
    "node-uuid": "1.4.7",
    "normalize-scss": "6.0.0",
    "parseurl": "1.3.1",
    "pug": "2.0.0-beta6",
    "rate-limit-redis": "1.1.0",
    "serve-favicon": "2.4.1",
    "socket.io": "1.7.3",
    "socket.io-client": "1.7.3",
    "socket.io-redis": "4.0.0",
    "spdy": "3.4.4",
    "static-asset": "0.6.0",
    "url": "0.11.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "brotli-webpack-plugin": "0.2.0",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.26.2",
    "eslint": "3.17.0",
    "eslint-plugin-pug": "1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "imagemin-cli": "3.0.0",
    "jasmine-node": "1.14.5",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "request": "2.80.0",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.2",
    "webpack": "1.14.0"
  }
}
