{
  "name": "express-security",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "npm run clean && npm-run-all -s build:*",
    "build:client": "cross-env BABEL_ENV=client webpack --progress --colors -p",
    "build:images": "imagemin ./client/images/* --out-dir ./server/public/images/",
    "build:server": "mkdirp build build/logs build/certificates && cross-env BABEL_ENV=server babel ./server --out-dir ./build --ignore public,views",
    "build:assets": "cp -r ./server/public ./server/config ./server/views ./server/certificates ./build",
    "clean": "npm-run-all -p clean:*",
    "clean:server": "rimraf ./build",
    "clean:client": "rimraf ./server/public/scripts ./server/public/styles",
    "clean:logs": "rimraf ./server/logs ./build/logs",
    "lint": "npm-run-all -s lint:*",
    "lint:client": "eslint ./client --ext .js --format codeframe",
    "lint:server": "eslint ./server --ext .js,.pug --format codeframe",
    "lint:tests": "eslint ./tests --ext .js --format codeframe",
    "start": "npm run build && node ./build/start.js",
    "test": "npm-run-all -s test:*",
    "test:unit": "cross-env BABEL_ENV=server mocha --slow 5 --check-leaks --full-trace --compilers js:babel-register ./tests/unit/",
    "test:e2e": "cross-env BABEL_ENV=server mocha --slow 150 --check-leaks --full-trace --compilers js:babel-register ./tests/e2e/",
    "test:vulnerabilities": "nsp check",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "mkdirp ./server/logs/ && nodemon ./server/start.js",
    "watch:client": "cross-env BABEL_ENV=client webpack --progress --colors --watch"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cache-manager": "2.9.0",
    "cache-manager-redis": "0.6.0",
    "compression": "1.7.3",
    "connect-redis": "3.3.3",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "3.1.0",
    "express": "4.16.3",
    "express-rate-limit": "2.11.0",
    "express-session": "1.15.6",
    "express-socket.io-session": "1.3.5",
    "extend": "3.0.2",
    "find": "0.2.9",
    "helmet": "3.13.0",
    "http-errors": "1.7.0",
    "jquery": "3.3.1",
    "mime-types": "2.1.19",
    "moment": "2.22.2",
    "morgan": "1.9.0",
    "ms": "2.1.1",
    "node-uuid": "1.4.8",
    "normalize-scss": "7.0.1",
    "pug": "2.0.3",
    "rate-limit-redis": "1.5.0",
    "serve-favicon": "2.5.0",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1",
    "socket.io-redis": "5.2.0",
    "spdy": "3.4.7",
    "static-asset": "0.6.0",
    "url": "0.11.0",
    "winston": "2.4.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "brotli-webpack-plugin": "0.5.0",
    "chai": "4.1.2",
    "compression-webpack-plugin": "1.1.11",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-mocha": "0.0.0",
    "eslint-config-semistandard": "12.0.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-mocha-only": "0.0.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-pug": "1.1.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "3.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "imagemin-cli": "3.0.0",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "node-sass": "4.9.2",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "postcss-loader": "2.1.6",
    "request": "2.87.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "set-cookie-parser": "2.2.1",
    "snyk": "1.89.1",
    "style-loader": "0.21.0",
    "supertest": "3.1.0",
    "webpack": "3.12.0"
  }
}
